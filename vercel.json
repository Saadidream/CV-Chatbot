{
    "version": 2,
    "framework": "create-react-app",
    "buildCommand": "cd frontend && npm install && npm run build",
    "outputDirectory": "frontend/build",
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "https://your-backend-url.com/api/$1"
      },
      {
        "src": "/static/(.*)",
        "dest": "/static/$1"
      },
      {
        "src": "/favicon.ico",
        "dest": "/favicon.ico"
      },
      {
        "src": "/manifest.json",
        "dest": "/manifest.json"
      },
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }